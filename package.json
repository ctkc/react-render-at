{
  "name": "react-render-at",
  "version": "1.1.0",
  "private": false,
  "description": "Render components dynamically based on the device",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctkc/react-render-at"
  },
  "scripts": {
    "build": "rm -rf ./dist && rollup -c",
    "build:types": "rm -rf ./dist && tsc --outDir dist/lib --declarationDir dist/types --declaration true && rollup -c",
    "files": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint ./src/** ./tests/** --ext .ts,.tsx,.js",
    "lint:fix": "eslint ./src/** ./tests/** --ext .ts,.tsx,.js --fix"
  },
  "author": {
    "name": "Juan Ignacio Settecase",
    "email": "settecasejuan@gmail.com"
  },
  "keywords": [
    "react",
    "render",
    "dynamic",
    "HOC",
    "hooks",
    "component"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.17.1",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "sinon": "^9.0.2",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/tests/mocks/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**"
    ],
    "setupFiles": [
      "<rootDir>/tests/setupTests.ts"
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
